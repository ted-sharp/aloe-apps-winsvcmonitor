      "provider": "jsdelivr",
      "library": "@picocss/pico@2.1.1",
      "destination": "wwwroot/lib/pico-css/",
      "files": ["css/pico.min.css"]
    },
    {
      "library": "open-props@1.7.17",
      "destination": "wwwroot/lib/open-props/",
